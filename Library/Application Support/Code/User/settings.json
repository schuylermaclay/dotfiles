{
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[liquid]": {
        "editor.defaultFormatter": "sissel.shopify-liquid"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "debug.javascript.autoAttachFilter": "always",
    "editor.detectIndentation": true,
    "editor.experimental.asyncTokenizationLogging": true,
    "editor.fontLigatures": false,
    "editor.fontWeight": 100,
    "editor.formatOnPaste": true,
    "editor.largeFileOptimizations": false,
    "editor.maxTokenizationLineLength": 2000000,
    "editor.minimap.enabled": false,
    "editor.minimap.autohide": false,
    "editor.minimap.showSlider": "mouseover",
    "editor.suggestSelection": "first",
    "prettier.printWidth": 120,
    "prettier.quoteProps": "consistent",
    "prettier.singleQuote": true,
    "prettier.tabWidth": 4,
    "search.exclude": {
        "**/node_modules": true,
        "**/build": true,
        "**/bower_components": true
    },
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.persistentSessionScrollback": 100000,
    "terminal.integrated.scrollback": 100000,
    "workbench.colorCustomizations": {},
    "workbench.colorTheme": "Default Dark+",
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.tree.enableStickyScroll": true,
    "workbench.tree.stickyScrollMaxItemCount": 10,
    "workbench.tree.expandMode": "singleClick",
    "editor.guides.indentation": true,
    "terminal.integrated.stickyScroll.enabled": true,
    "terminal.integrated.stickyScroll.maxLineCount": 2,
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "/opt/homebrew/bin/bash",
            "args": ["-l"],
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "zsh",
            "args": ["-l"]
        },
        "fish": {
            "path": "/opt/homebrew/bin/fish",
            "args": ["-l"]
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        }
    },
    "terminal.integrated.defaultProfile.osx": "fish",
    "editor.acceptSuggestionOnEnter": "on",
    "editor.quickSuggestionsDelay": 5,
    "editor.formatOnSave": true,
    "editor.fontFamily": "'CaskaydiaCove Nerd Font','FuraCode Nerd Font', Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.gotoLocation.multipleDefinitions": "goto",
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html-hubl]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "hubspot.beta": true,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "workbench.editorAssociations": {
        "*.html": "default"
    },
    "Lua.workspace.library": ["/Users/schuylermaclay/.hammerspoon/Spoons/EmmyLua.spoon/annotations"],
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.minimap.renderCharacters": false,
    "editor.minimap.maxColumn": 80,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.enablePreview": false,
    "terminal.integrated.fontWeight": "200",
    "terminal.integrated.fontWeightBold": "bold",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "[scss]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "gitlens.outputLevel": "error",
    "files.associations": {
        ".ripgreprc": "ini"
    },
    "editor.tokenColorCustomizations": {
        "[Monokai Pro]": {
            "comments": "#697573"
        }
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "multiCommand.commands": {
        "multiCommand.runLastTerminalCommand": {
            "key": "alt+x",
            "command": "extension.multiCommand.execute",
            "args": {
                "sequence": [
                    "workbench.action.files.saveAll",
                    "workbench.action.terminal.scrollToBottom",
                    {
                        "command": "workbench.action.terminal.sendSequence",
                        "args": { "text": "\u001b[A\u000D" }
                    },
                    "workbench.action.focusActiveEditorGroup"
                ]
            },
            "when": "editorFocus"
        }
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "github.copilot.editor.enableAutoCompletions": true,
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "emmet.includeLanguages": {
        "html-hubl": "html"
    },
    "svelte.enable-ts-plugin": true,
    "[svelte]": {
        "editor.defaultFormatter": "svelte.svelte-vscode"
    },
    "shellcheck.executablePath": "shellcheck",
    "shellcheck.customArgs": ["-x"],
    "shellcheck.ignorePatterns": {
        "**/*.zsh": true,
        "**/*.zshrc": true,
        "**/zshrc": true,
        "**/*.zshenv": true,
        "**/zshenv": true,
        "**/*.zprofile": true,
        "**/zprofile": true
    },
    "editor.stickyScroll.maxLineCount": 10,
    "editor.stickyScroll.defaultModel": "indentationModel",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
